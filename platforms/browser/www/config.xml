<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.rainetcom.workandpadel" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>WORK AND PADEL</name>
    <description>
        Réservez vos terrains, ajoutez-vous à des parties et participez aux animations
    </description>
    <author email="contact@rainetcom.fr" href="http://www.rainetcom.fr">
        Team RainetCom
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="http://*" />
    <access origin="https://*" />
    <config-file mode="replace" parent="NSPhotoLibraryUsageDescription" platform="ios">
        <string>L'application aimerait accéder à la galerie pour vous permettre de modifier votre photo de profil</string>
    </config-file>
    <config-file mode="replace" parent="NSCameraUsageDescription" platform="ios">
        <string>L'application aimerait accéder à la caméra pour vous permettre de modifier votre photo de profil</string>
    </config-file>
    <gap:config-file parent="NSPhotoLibraryUsageDescription" platform="ios">
        <string>L'application aimerait accéder à la galerie pour vous permettre de modifier votre photo de profil</string>
    </gap:config-file>
    <gap:config-file overwrite="true" parent="NSCameraUsageDescription" platform="ios">
        <string>L'application aimerait accéder à la camera pour pourvoir vous permettre de modifier votre photo de profil</string>
    </gap:config-file>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>L'application aimerait accéder à la galerie pour vous permettre de modifier votre photo de profil</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
        <string>L'application aimerait accéder à la caméra pour vous permettre de modifier votre photo de profil</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
        <string>need location access to find things nearby</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryAddUsageDescription">
        <string>need photo library access to save pictures there</string>
    </edit-config>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="20" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="phonegap-version" value="cli-9.0.0" />
</widget>
